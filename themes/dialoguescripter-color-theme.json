{
  "name": "DialogueScripter Theme",
  "type": "dark",
  "colors": {},
  "tokenColors": [
    {
      "scope": "keyword.control.ds",
      "settings": { "foreground": "#569CD6" }
    },
    {
      "scope": "string.unquoted.path.ds",
      "settings": { "foreground": "#E0E0E0" }
    },
    {
      "scope": "entity.name.label.ds",
      "settings": { "foreground": "#FFFF00" }
    },
    {
      "scope": "entity.name.speaker.ds",
      "settings": { "foreground": "#4EC9B0" }
    },
    {
      "scope": "entity.name.tag.ds",
      "settings": { "foreground": "#C00000" }
    },
    {
      "scope": "constant.numeric.ds",
      "settings": { "foreground": "#B5CEA8" }
    },
    {
      "scope": "constant.language.boolean.ds",
      "settings": { "foreground": "#569CD6" }
    },
    {
      "scope": "variable.other.ds",
      "settings": { "foreground": "#9CDCFE" }
    },
    {
      "scope": "string.quoted.double.ds",
      "settings": { "foreground": "#CE9178" }
    },
    {
      "scope": "comment.line.number-sign.ds",
      "settings": { "foreground": "#6A9955" }
    },
    {
      "scope": "entity.name.function.ds, entity.name.function.embedded.ds",
      "settings": { "foreground": "#DCDCAA" }
    }
  ]
}