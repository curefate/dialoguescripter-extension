{
  "name": "dialoguescripter",
  "displayName": "DialogueScripter",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./out/lsp/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "ds",
        "aliases": [
          "DS script",
          "ds"
        ],
        "extensions": [
          ".ds"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ds",
        "scopeName": "source.ds",
        "path": "./syntaxes/ds.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "label": "DialogueScripter Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/dialoguescripter-color-theme.json"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "18.16.19",
    "@types/vscode": "1.102.0",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "vscode-languageclient": "7.0.0",
    "vscode-languageserver": "7.0.0",
    "vscode-languageserver-textdocument": "1.0.8"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  }
}
